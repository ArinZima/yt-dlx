{
  "name": "yt-dlx",
  "version": "2.0.4",
  "types": "shared/index.d.ts",
  "main": "shared/index.cjs.js",
  "module": "shared/index.esm.js",
  "description": "Effortless audio-video download and streaming. yt-dlx does the heavy load for free.",
  "bin": {
    "yt": "shared/cli/main.js",
    "yt-dlx": "shared/cli/main.js"
  },
  "scripts": {
    "start": "node .devcontainer/startup.mjs",
    "setup": "chmod +x ./linux-setup.sh && ./linux-setup.sh",
    "remake": "bun run clean && bun run make && bun run build",
    "remake:update": "bun run clean && bun run make && bun install --latest && bun run build",
    "upload": "bun run test && bun run cli && bun run remake && npm pkg fix && bun run publish --access=public && bun run update",
    "clean": "bun run clean:base && bun run clean:frontend",
    "clean:base": "rm -rf node_modules temp shared bun.lockb",
    "clean:frontend": "cd frontend && rm -rf node_modules .next bun.lockb",
    "make": "bun run make:base && bun run make:frontend",
    "make:base": "bun install",
    "make:frontend": "cd frontend && bun install",
    "build": "bun run build:base && bun run build:frontend",
    "build:base": "tsup --config 'tsup.config.ts' && rollup -c 'rollup.config.mjs'",
    "build:frontend": "cd frontend && bun run build",
    "update": "bun run make && bun run update:base && bun run update:frontend",
    "update:base": "bun install --latest",
    "update:frontend": "cd frontend && bun install --latest",
    "cli": "bun run link && bun run test:cli && bun run unlink",
    "spec": "vitest --config 'vitest.config.mts'",
    "test": "bun run test:bun && bun run test:cli",
    "test:bun": "bun test --timeout 60000 --bail --watch",
    "test:cli": "yt version && yt-dlx audio-lowest --query 'PERSONAL BY PLAZA' && yt-dlx al --query 'SuaeRys5tTc'"
  },
  "files": [
    "util",
    "shared",
    "LICENSE",
    "README.md",
    "yt-dlx.png",
    "package.json"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "yt-dlx"
  },
  "licenses": [
    {
      "type": "AGPL-3.0",
      "url": "https://opensource.org/licenses/AGPL-3.0"
    }
  ],
  "homepage": "https://rebrand.ly/mixly",
  "repository": {
    "type": "git",
    "url": "https://mixly-yt-dlx.koyeb.app/pkg"
  },
  "keywords": [
    "yt-dlx",
    "ytdlp",
    "cli",
    "yt"
  ],
  "dependencies": {
    "async-retry": "^1.3.3",
    "axios": "^1.6.7",
    "bun": "latest",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "minimist": "^1.2.8",
    "puppeteer": "^22.2.0",
    "readline-sync": "^1.4.10",
    "spinnies": "^0.5.1",
    "stream": "^0.0.2",
    "vitest": "latest",
    "yt-dlx": "latest",
    "yt-search": "^2.10.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/async": "latest",
    "@types/async-retry": "latest",
    "@types/bun": "latest",
    "@types/cheerio": "latest",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "latest",
    "@types/readline-sync": "^1.4.8",
    "@types/rollup-plugin-progress": "^1.1.4",
    "@types/spinnies": "latest",
    "@types/yt-search": "^2.10.3",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "latest"
  }
}
