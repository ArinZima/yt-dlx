{
  "name": "yt-dlx",
  "version": "1.0.2",
  "types": "proto/index.d.ts",
  "main": "proto/index.cjs.js",
  "module": "proto/index.esm.js",
  "description": "Effortless audio-video download and streaming. yt-dlx does the heavy load for free.",
  "bin": {
    "yt": "proto/cli/main.js",
    "yt-dlx": "proto/cli/main.js"
  },
  "scripts": {
    "ingress": "cd backend && yarn ingress",
    "start": "node .devcontainer/startup.mjs",
    "setup": "chmod +x ./linux-setup.sh && ./linux-setup.sh",
    "remake": "yarn clean && yarn make && yarn build",
    "remake_update": "yarn clean && yarn make && yarn update && yarn build",
    "upload": "yarn test && yarn cli && yarn remake && npm pkg fix && yarn publish --access=public && yarn update",
    "clean": "yarn clean_base && yarn clean_backend && yarn clean_frontend",
    "clean_base": "rm -rf node_modules temp proto yarn.lock",
    "clean_backend": "cd backend && rm -rf node_modules temp proto yarn.lock",
    "clean_frontend": "cd frontend && rm -rf node_modules .next yarn.lock",
    "make": "yarn make_base && yarn make_backend && yarn make_frontend",
    "make_base": "yarn install",
    "make_backend": "cd backend && yarn install",
    "make_frontend": "cd frontend && yarn install",
    "build": "yarn build_base && yarn build_backend && yarn build_frontend",
    "build_base": "tsup --config 'tsup.config.ts' && rollup -c 'rollup.config.mjs'",
    "build_backend": "cd backend && rollup -c 'rollup.config.mjs'",
    "build_frontend": "cd frontend && yarn build",
    "update": "yarn make && yarn update_base && yarn update_backend && yarn update_frontend",
    "update_base": "yarn upgrade --latest",
    "update_backend": "cd backend && yarn upgrade --latest",
    "update_frontend": "cd frontend && yarn upgrade --latest",
    "spec": "tsup 'delta/quick.spec.ts' --outDir 'temp' && node 'temp/quick.spec.js'",
    "test": "yarn ingress & yarn test_base && yarn test_backend && yarn test_scrape && yarn cli",
    "test_base": "tsup 'delta/__tests__' --outDir 'temp' && node 'temp/cjs.mjs'",
    "test_scrape": "tsup 'delta/scrape.spec.ts' --outDir 'temp' && node 'temp/scrape.spec.mjs'",
    "test_backend": "tsup 'delta/ytcprox.spec.ts' --outDir 'temp' && node 'temp/ytcprox.spec.mjs'",
    "cli": "yarn link && yarn cli_test && yarn unlink",
    "cli_test": "yt version && yt-dlx audio-lowest --query 'PERSONAL BY PLAZA' && yt-dlx al --query 'SuaeRys5tTc'"
  },
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "shovitdutta"
  },
  "licenses": [
    {
      "type": "AGPL-3.0",
      "url": "https://opensource.org/licenses/AGPL-3.0"
    }
  ],
  "homepage": "https://rebrand.ly/mixly",
  "repository": {
    "type": "git",
    "url": "https://mixly-shovitdutta.koyeb.app/pkg"
  },
  "keywords": [
    "yt-dlx",
    "ytdlp",
    "cli",
    "yt"
  ],
  "dependencies": {
    "async-retry": "^1.3.3",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "dotenv": "^16.4.4",
    "fluent-ffmpeg": "^2.1.2",
    "minimist": "^1.2.8",
    "package-json": "^9.0.0",
    "playwright": "^1.41.2",
    "readline-sync": "^1.4.10",
    "stream": "^0.0.2",
    "yt-search": "^2.10.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/async": "^3.2.24",
    "@types/chai": "^4.3.11",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.11.19",
    "@types/readline-sync": "^1.4.8",
    "@types/rollup-plugin-progress": "^1.1.4",
    "@types/yt-search": "^2.10.3",
    "chai": "^5.1.0",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
