{
  "name": "yt-dlp",
  "version": "20.1.0",
  "types": "core/index.d.ts",
  "main": "core/index.cjs.js",
  "module": "core/index.esm.js",
  "description": "Effortless audio-video download and streaming. yt-dlp does the heavy load for free.",
  "bin": {
    "yt": "core/cli/main.js",
    "yt-dlp": "core/cli/main.js"
  },
  "scripts": {
    "clean": "yarn run clean:base && yarn run clean:source && yarn run clean:frontend",
    "clean:base": "rm -rf node_modules temp core yarn.lock",
    "clean:source": "cd source && rm -rf node_modules temp core yarn.lock",
    "clean:frontend": "cd frontend && rm -rf node_modules .next yarn.lock",
    "build": "yarn run build:base && yarn run build:source && yarn run build:frontend",
    "build:base": "tsup --config 'tsup.config.ts' && rollup -c 'rollup.config.mjs'",
    "build:source": "cd source && rollup -c 'rollup.config.mjs'",
    "build:frontend": "cd frontend && yarn run build",
    "make": "yarn run make:base && yarn run make:source && yarn run make:frontend",
    "make:base": "yarn run install",
    "make:source": "cd source && yarn run install",
    "make:frontend": "cd frontend && yarn run install",
    "update": "yarn run update:base && yarn run update:source && yarn run update:frontend",
    "update:base": "yarn upgrade --latest",
    "update:source": "cd source && yarn upgrade --latest",
    "update:frontend": "cd frontend && yarn upgrade --latest",
    "remake": "yarn run clean && yarn run make && yarn run update",
    "setup": "chmod +x .devcontainer/setup.sh && .devcontainer/setup.sh",
    "server": "cd source && yarn run start",
    "spec": "tsup 'app/quick.spec.ts' --outDir 'temp' && node 'temp/quick.spec.mjs'",
    "test": "yarn run test:base && yarn run test:source && yarn run test:scrape",
    "test:base": "yarn run server & tsup 'app/__tests__' --outDir 'temp' && node 'temp/cjs.mjs'",
    "test:source": "yarn run server & tsup 'app/play.spec.ts' --outDir 'temp' && node 'temp/play.spec.mjs'",
    "test:scrape": "yarn run server & tsup 'app/scrape.spec.ts' --outDir 'temp' && node 'temp/scrape.spec.mjs'",
    "cli": "yarn run link && yarn run cli:test && yarn run unlink",
    "cli:test": "yt version && yt-dlp audio-lowest --query 'PERSONAL BY PLAZA' && yt-dlp al --query 'SuaeRys5tTc'",
    "upload": "yarn run test && yarn run cli && yarn run remake && npm pkg fix && yarn run publish --access=public && yarn run update"
  },
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "shovitdutta"
  },
  "licenses": [
    {
      "type": "AGPL-3.0",
      "url": "https://opensource.org/licenses/AGPL-3.0"
    }
  ],
  "homepage": "https://rebrand.ly/mixly",
  "repository": {
    "type": "git",
    "url": "https://mixly-shovitdutta.koyeb.app/pkg"
  },
  "keywords": [
    "yt-dlp",
    "ytdlp",
    "cli",
    "yt"
  ],
  "dependencies": {
    "@shovit/ytid": "^1.0.0",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "dotenv": "^16.4.4",
    "fluent-ffmpeg": "^2.1.2",
    "minimist": "^1.2.8",
    "package-json": "^9.0.0",
    "playwright": "^1.41.2",
    "readline-sync": "^1.4.10",
    "stream": "^0.0.2",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/async": "^3.2.24",
    "@types/chai": "^4.3.11",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.11.19",
    "@types/readline-sync": "^1.4.8",
    "@types/rollup-plugin-progress": "^1.1.4",
    "@types/yt-search": "^2.10.3",
    "chai": "^5.1.0",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
